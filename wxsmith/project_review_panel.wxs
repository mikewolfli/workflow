<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxPanel" name="project_review_panel">
		<pos_arg>1</pos_arg>
		<size_arg>1</size_arg>
		<object class="wxBoxSizer" variable="BoxSizer1" member="no">
			<orient>wxVERTICAL</orient>
			<object class="sizeritem">
				<object class="wxBoxSizer" variable="BoxSizer2" member="no">
					<object class="sizeritem">
						<object class="wxButton" name="ID_BUTTON4" variable="Button_Communication" member="yes">
							<label>评审问题反馈</label>
							<handler function="OnButton_CommunicationClick" entry="EVT_BUTTON" />
						</object>
						<flag>wxALL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxButton" name="ID_BUTTON1" variable="Button_Finish" member="yes">
							<label>完成</label>
							<handler function="OnButton_FinishClick" entry="EVT_BUTTON" />
						</object>
						<flag>wxALL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxButton" name="ID_BUTTON2" variable="Button_Return" member="yes">
							<label>退回</label>
							<handler function="OnButton_ReturnClick" entry="EVT_BUTTON" />
						</object>
						<flag>wxALL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxButton" name="ID_BUTTON3" variable="Button_Refresh" member="yes">
							<label>刷新任务</label>
							<handler function="OnButton_RefreshClick" entry="EVT_BUTTON" />
						</object>
						<flag>wxALL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
				</object>
				<flag>wxALL|wxALIGN_LEFT</flag>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxStaticBoxSizer" variable="StaticBoxSizer1" member="no">
					<label>任务清单</label>
				</object>
				<flag>wxALL|wxEXPAND</flag>
				<option>5</option>
			</object>
		</object>
		<object class="wxMenu" variable="menu_review_log" member="yes">
			<object class="wxMenuItem" name="idMenu_process" variable="mi_process" member="yes">
				<label>项目评审流程信息(&amp;I)</label>
				<help>项目评审流程信息</help>
				<handler function="Onmi_processSelected" entry="EVT_MENU" />
			</object>
			<object class="wxMenuItem" name="idMenu_draw_qty" variable="mi_draw_qty" member="yes">
				<label>更改图纸套数(&amp;D)</label>
				<help>更改图纸套数</help>
				<handler function="Onmi_draw_qtySelected" entry="EVT_MENU" />
			</object>
			<object class="wxMenuItem" name="idMenu_restart_log" variable="mi_review_history" member="yes">
				<label>重启评审历史(&amp;H)</label>
				<help>重启评审历史</help>
				<handler function="Onmi_review_historySelected" entry="EVT_MENU" />
			</object>
			<object class="wxMenuItem" name="idMenu_Communication_log" variable="mi_com_log" member="yes">
				<label>问题传递log(&amp;L)</label>
				<help>问题传递log</help>
				<handler function="Onmi_com_logSelected" entry="EVT_MENU" />
			</object>
			<object class="wxMenuItem" name="idMenu_pos" variable="mi_pos" member="yes">
				<label>快速定位(&amp;P)</label>
				<accel>Ctrl+F</accel>
				<help>快速定位</help>
				<handler function="Onmi_posSelected" entry="EVT_MENU" />
			</object>
		</object>
	</object>
</wxsmith>
